{
  "name": "tamia",
  "description": "Tiny but extremely opinionated front-end framework",
  "version": "3.0.0-aplha.5",
  "scripts": {
    "test": "npm run lint",
    "start": "webpack-dev-server --inline --content-base test/specs --config config/webpack.config.js --host 0.0.0.0 --no-info",
    "prepublish": "npm run compile",
    "mocha": "mocha test src/**/*.spec.js",
    "mocha:watch": "mocha --watch --reporter min test src/**/*.spec.js",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "eslint . --fix --ext .js,.jsx",
    "lint:css": "stylelint '**/*.pcss'",
    "compile": "npm run compile:clean && npm run compile:copy && npm run compile:babel",
    "compile:clean": "rimraf lib",
    "compile:copy": "cpy '**/*.pcss' '../lib' --cwd src --parents",
    "compile:babel": "babel -d lib/ src/",
    "compile:watch": "babel --watch -d lib/ src/",
    "docs": "npm run docs:api && npm run docs:bundle && npm run docs:build && npm run docs:publish",
    "docs:api": "mkdirp docs/md && documentation build -c docs/src/config.json --format md --output docs/md/api.md src/*.js",
    "docs:bundle": "webpack --config config/webpack.docs.config.js --no-info",
    "docs:build": "babel-node docs/src",
    "docs:publish": "gh-pages -d docs/public"
  },
  "homepage": "http://tamiadev.github.io/tamia/",
  "author": {
    "name": "Artem Sapegin",
    "url": "http://sapegin.me/"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/tamiadev/tamia.git"
  },
  "bugs": {
    "url": "https://github.com/tamiadev/tamia/issues"
  },
  "license": "MIT",
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "engines": {
    "node": ">=7"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "document-register-element": "^0.5.4",
    "html-attrs": "^1.0.0",
    "lodash": "^4.17.4",
    "social-share-services": "^1.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.5",
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.3.2",
    "babel-plugin-css-modules-transform": "^1.2.1",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-react-jsx": "^6.23.0",
    "babel-preset-env": "^1.1.10",
    "babel-preset-latest": "^6.22.0",
    "chai": "^3.5.0",
    "cpy-cli": "^1.0.1",
    "css-loader": "^0.26.2",
    "documentation": "4.0.0-beta15",
    "domino": "^1.0.27",
    "eslint": "^3.16.1",
    "eslint-config-tamia": "^3.3.2",
    "eslint-plugin-react": "^6.10.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "fledermaus": "^4.0.0",
    "fs-extra": "^2.0.0",
    "gh-pages": "^0.12.0",
    "glob": "^7.1.1",
    "highlight.js": "^9.9.0",
    "mkdirp": "^0.5.1",
    "mocha": "^2.4.5",
    "postcss-loader": "^1.3.2",
    "rimraf": "^2.6.1",
    "sequelize": "^3.25.0",
    "sinon": "^1.17.6",
    "sqlite3": "^3.1.8",
    "style-loader": "^0.13.2",
    "stylelint": "^7.9.0",
    "stylelint-config-standard": "^16.0.0",
    "underscore.string": "^3.3.4",
    "vdo": "^4.1.0",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.2",
    "webpack-merge": "^3.0.0"
  }
}
